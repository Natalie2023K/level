	<link href='css/dashboard.css' rel='stylesheet' />
	 <meta name="viewport" content="width=device-width, initial-scale=1" />

</head>

<body>
<div class="page page--dashboard">
	<aside class="page_aside aside vertical vertical--big">
		<div class="text aside_logo">Sidebar</div>
		<nav class="vertical vertical--small">
			<a href="#" class="nav-button nav-button--active click">
				<span class="icon nav-button_icon" style="background-image:url('/svg/nav/home.svg')"></span>
				<span class="text  text--light">Home</span>
			</a>
			<a href="#" class="nav-button  click">
				<span class="icon nav-button_icon" style="background-image:url('/svg/nav/order.svg')"></span>
				<span class="text  text--light">Orders</span>
			</a>
			<a href="#" class="nav-button  click">
				<span class="icon nav-button_icon" style="background-image:url('/svg/nav/products.svg')"></span>
				<span class="text  text--light">Products</span>
			</a>
		</nav>
	</aside>
	<header class="header">
		<div class="layout header_content text--small">
			<div class="header_search relative">
				<input type="text" placeholder="Search" class="text field field--header">
				<span class="icon header_icon-search"  style="background-image:url('/svg/header/search.svg')"></span>
				<span class="icon icon--header-settings icon--button icon--color-blue click" style="background-image:url('/svg/header/settings.svg')"></span>
				
			</div>
			<div class="header_icons">
					<span class="icon header-button_icon icon--yellow" style="background-image:url('/svg/header/antenna.svg')"></span>
					<span class="header-button_icon icon--blue" style="background-image:url('/svg/header/symbols.svg')"></span>
					<span class=" header-button_icon icon--yellow" style="background-image:url('/svg/header/bell.svg')"></span>
				</div>
		</div>
		
	</header>
	<main class="main">
		<div class="layout">
			<section class="box analytic_block">
				<div class="analytic anlytic--double analytic--purple">
						<span class="icon analytic_button icon--button icon--color-purple click" 
						style="background-image:url('/svg/analytic/money.svg')"></span>
						<div class=" relative vertical vertical--small">
							<div class="analytic_title text text--light">$500.00</div>
							<div class=" text text--small text--lighter">Total earning</div>
							<span class="icon analytic_arrow " 
						style="background-image:url('/svg/analytic/blueArrow.svg')"></span>
						</div>
				</div>
				<div class="analytic anlytic--double analytic--primary">
					<span class="icon analytic_button icon--button icon--color-primary click" 
						style="background-image:url('/svg/analytic/shopp.svg')"></span>
						<div class=" relative vertical vertical--small">
							<div class="analytic_title text text--light">$961</div>
							<div class=" text text--small text--lighter">Total Order</div>
							
						</div>
				</div>
				<div class="analytic analytic--row analytic--blue">
               <span class="icon analytic_button icon--button icon--color-primary click" 
						style="background-image:url('/svg/analytic/money.svg')"></span>
						<div class="relative vertical vertical--small">
							<div class="analytic_title text text--light">$203k</div>
							<div class=" text text--small text--lighter">Total Income</div>
						</div>
				</div>
				<div class="analytic analytic--row analytic--white">
                <span class="icon analytic_button icon--button icon--yellow click" 
						style="background-image:url('/svg/analytic/shop2.svg')"></span>
						<div class="relative vertical vertical--small">
							<div class="analytic_title text ">$203k</div>
							<div class=" text text--small ">Total Income</div>
				</div>
				
			</section>
			<section class="purchase">

			</section>
		</div>
	</main>
</div>

	

</body>

</html>