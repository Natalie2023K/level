<header class="header">
			
				{{>logo}}
			

			<nav class="header_nav">
          <div class="header_nav_right">
	      {{>nav list=header}}
           </div>
			</nav>
			</header>
			{{#with main}}
			<div class="main">
				<img src="{{image}}" alt="Hero image" class="img_main">
				<div class="main_content">
					<h1 class="main_title">{{heading}}</h1>
					<p class="main_text">{{descriotion}}</p>
					<a href="{{href}}" class="main_button">{{button}}</a>
				</div>
				{{/with}}
				</div>
				<main class="featured">
					{{#each featured}}
					<div class="card">
						<div class="card_content">
							<img src="{{photo}}" alt="card image" class="card_image">
						<h2 class="card_title">{{heading}}</h2>
						<p class="card_text">{{info}}</p>
						</div>
						<a href="{{linkUrl}}" class="card_link">{{linkText}}</a>

					</div>
					{{/each}}
				</main>
				<div class="actions">
					{{#each actions}}
					<div class="actions_content">
						<h3 class="action_title">{{heading}}</h3>
					<a href="{{url}}" class="action_button">{{text}}</a>
					</div>
					{{/each}}
				</div>
				

			
			
			

		
		<footer class="footer">
			<div class="">
             {{>logo}}
           <span class="footer_copyright">Copyright 2023-2024</span>
		   
			</div>
			{{#each footer}}
			<div class="">
				<span class="footer_title">{{title}}</span>
				{{>nav list=links isVertical=true}}
			</div>
			{{/each}}
		</footer>
	