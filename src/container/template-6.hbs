<header class="header">
			
				{{>logo}}
			

			<nav class="header_nav">
          <div class="header_nav_right">
	      {{>nav list=header}}
           </div>
			</nav>
			</header>
			<main class="main">
				<div class="info">
					{{#with info}}
					<h1 class="info_title">{{title}}</h1>
					<p class="info_description">{{description}}</p>
					{{/with}}
				</div>
				

				{{#with form}}
				<form class="form">
					<h2 class="form_title">{{title}}</h2>
					<div class="form_input_list">
						{{#each inputs}}
						<input type={{type}} placeholder="{{placeholder}}" name="{{name}}"  id="form_input-{{id}}" class="form_input">
						
						{{/each}}
					</div>
					<div class="form_checkbox">
						<input type="checkbox" name="{{checkbox.name}} id={{checkbox.name}}">
						<div></div>
						<label form="checkbox.name">{{checkbox.label}}</label>
					</div>
					<button type="submit" class="form_button">{{button}}</button>
					<p class="form_info">{{info}}</p>
				</form>
				{{/with}}
			</main>
			<div class="updates">
					{{#with updates}}
					<h1 class="updates_title">{{title}}</h1>
					<div class="updates_list">
						
							{{#each list}}
                           <div class="upd_list_card">
                          <div class="img_updates">32x32</div>
						  <div class="updates_text">
							<h2 class="list_nickname">{{nickname}}</h2>
							<p class="list_description">{{description}}</p>
							
						  
						   </div>
						</div>
						{{/each}}
						<a href="{{show_all.utrl}}" class="show_all">{{show_all.text}}</a>
						
						
					</div>
					{{/with}}
				</div>
				<div class="exp">
					{{#with expirience}}
					<strong class="exp_title">{{title}}</strong>
					<div class="badje">
						<h4>{{badge}}</h4>
					</div>

					<div class="exp_content">
						{{#each list}}
					<div class="exp_text">
						<h3 class="exp_name">{{name}}</h3>
					<p class="exp_det">{{details}}</p>
					</div>
					{{/each}}
					</div>
					{{/with}}
				</div>
			


		<footer class="footer">
			<div class="">
             {{>logo}}
           <span class="footer_copyright">Copyright 2023-2024</span>
		   
			</div>
			{{#each footer}}
			<div class="">
				<span class="footer_title">{{title}}</span>
				{{>nav list=links isVertical=true}}
			</div>
			{{/each}}
		</footer>